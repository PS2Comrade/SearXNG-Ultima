{
  "manifest_version": 2,
  "version": "1.0",
  "name": "SearXNG (search.demoniak.ch)",
  "short_name": "SearXNG",
  "author": "ComradePS2",
  "description": "Add SearXNG to search engine list.",
  "icons": {
    "16": "images/searxng-wordmark.svg",
    "128": "images/searxng-wordmark.svg"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "SearXNG",
      "keyword": "searx, searxng",
      "search_url": "https://search.demoniak.ch/search?q={searchTerms}",
      "favicon_url": "images/searxng-wordmark.svg",
      "suggest_url": "https://search.demoniak.ch/autocompleter?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"]
  },
  "storage": {
    "name": "local",
    "description": "Store search provider settings"
  },
  "storage": {
    "name": "local",
    "description": "Store search provider settings"
  }
}
