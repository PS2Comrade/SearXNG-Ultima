{
  "manifest_version": 3,
  "name": "SearXNG",
  "version": "1.1",
  "description": "Add SearXNG (search.im-in.space instance to search engine list.",
  "icons": {
    "16": "images/searxng-wordmark.svg",
    "128": "images/searxng-wordmark.svg"
  },
  "permissions": ["search"],
  "chrome_url_overrides": {
    "search_provider": {
      "name": "SearXNG",
      "keyword": "searx, searxng",
      "search_url": "https://search.im-in.space/search?q={searchTerms}",
      "suggest_url": "https://search.im-in.space/autocompleter?q={searchTerms}",
      "encoding": "UTF-8"
    }
  }
}
