{
  "manifest_version": 2,
  "version": "1.0",
  "name": "SearXNG (search.demoniak.ch)",
  "short_name": "SearXNG",
  "author": "thedoggybrad",
  "description": "Add SearXNG (search.demoniak.ch instance) to search engine list.",
  "icons": {
    "16": "images/searxng-wordmark.svg",
    "128": "images/searxng-wordmark.svg"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "SearXNG",
      "keyword": "searx, searxng",
      "search_url": "",
      "favicon_url": "images/searxng-wordmark.svg",
      "suggest_url": "",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["options.js"]
    }
  ]
}
